{
  "manifest_version": 3,
  "name": "YouTube Speed Controller",
  "version": "1.0",
  "description": "Control YouTube playback speed with [ and ] keys.",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
